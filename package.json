{
  "name": "mashroom",
  "version": "1.0.0",
  "private": true,
  "description": "mashroom",
  "author": "yoshinori.matsuzaki",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type:check": "tsc",
    "lint": "eslint --max-warnings=0 --ext js,jsx,ts,tsx src",
    "lint:fix": "eslint --max-warnings=0 --fix --ext js,jsx,ts,tsx src",
    "prepare": "husky install",
    "lint-staged": "lint-staged --concurrent=false --config .lintstagedrc.js"
  },
  "lint-staged": {
    "src/**/!(.)*.{js,jsx,ts,tsx}": [
      "yarn lint:fix"
    ],
    "src/**/!(.)*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn format:fix"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "gatsby": "^4.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby-plugin-image": "^2.7.0",
    "gatsby-plugin-manifest": "^4.7.0",
    "gatsby-plugin-mdx": "^3.7.1",
    "gatsby-plugin-react-helmet": "^5.7.0",
    "gatsby-plugin-sharp": "^4.7.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-source-filesystem": "^4.7.0",
    "gatsby-transformer-sharp": "^4.7.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
